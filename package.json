  "license": "MIT",
  "main": "bin/index.min.js",
  "types": "bin/types/index.d.ts",
  "scripts": {
    "build": "rm -rf bin && npm run build:types && npm run build:bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".tsx\" --ignore \"**/*.test.tsx\" --source-maps inline",
    "build:bundle": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --config ./example/webpack.config.js --mode development --open --hot",
    "test": "jest --watchAll",
    "test:once": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run build && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "react",
    "component"
  ],
  "peerDependencies": {
    "react": "^16.8.x",
    "react-dom": "^16.8.x"
  },
